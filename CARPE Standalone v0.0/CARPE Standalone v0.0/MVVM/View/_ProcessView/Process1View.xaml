             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <TextBlock Text="Processor (1/3)"
                   Foreground="White"
                   FontSize="28"
                   HorizontalAlignment="Left"
                   Margin="10,0,0,20" />
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal" RenderTransformOrigin="0.691,0.515" Margin="40,10,0,0" Height="30">
            <Border Width="100">
                <TextBlock Text="Input Path"
                           Foreground="White"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Center"
                           Margin="10,0,0,0" />
            </Border>
            <Border>
                <TextBox Width="400"
                         Height="36"
                         x:Name="source_input"
                         Text="{Binding Path=src_input,UpdateSourceTrigger=PropertyChanged}"
                         VerticalAlignment="Center"
                         HorizontalAlignment="Left"
                         Margin="10,0,10,-6"
                         Grid.Column="1"
                         Style="{StaticResource ModernInputbox}"/>
            </Border>
            <Border>
                <Button x:Name="path1" Command="{Binding OpenfileClick}"
                        BorderThickness="0"
                        Style="{StaticResource ModernButton}" Width="41" Margin="0,-7,0,-6" RenderTransformOrigin="0.5,0.244" />
            </Border>
        </StackPanel>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal" RenderTransformOrigin="0.691,0.515" Margin="40,10,0,0" Height="30">
            <Border Width="100">
                <TextBlock Text="Output Path"
                           Foreground="White"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Center"
                           Margin="10,0,0,0" />
            </Border>
            <Border>
                <TextBox Width="400"
                         x:Name="output_input"
                         Height="36"
                         Text="{Binding Path= output_input,UpdateSourceTrigger=PropertyChanged}"
                         VerticalAlignment="Center"
                         HorizontalAlignment="Left"
                         Margin="10,1,10,-6"
                         Grid.Column="1"
                         Style="{StaticResource ModernInputbox}" />
            </Border>
            <Border>
                <Button x:Name="path2"
                        BorderThickness="0"
                        Command="{Binding OpenpathClick}"
                        Style="{StaticResource ModernButton}" Width="42" Margin="0,-4,0,-12" />
            </Border>
        </StackPanel>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal" RenderTransformOrigin="0.691,0.515" Margin="0,10,0,0" Height="30">
            <Border Width="100">
                <TextBlock Text="Description"
                           Foreground="White"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Center"
                           Margin="10,0,0,0" />
            </Border>
            <Border>
                <TextBox Width="400"
                         x:Name="description"
                         Height="40"
                         Text="{Binding Path=description,UpdateSourceTrigger=PropertyChanged}"
                         VerticalAlignment="Center"
                         HorizontalAlignment="Left"
                         Margin="10,1,10,-6"
                         Grid.Column="1"
                         Style="{StaticResource ModernInputbox}" />
            </Border>

        </StackPanel>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal" RenderTransformOrigin="0.691,0.515" Margin="0,10,0,0" Height="30">
            <Border Width="100">
                <TextBlock Text="Case Name"
                           Foreground="White"
                           VerticalAlignment="Center"
                           HorizontalAlignment="Center"
                           Margin="10,0,0,0" />
            </Border>
            <Border>
                <TextBox Width="400"
                         x:Name="CaseName"
                         Height="40"
                         Text="{Binding Path= casename,UpdateSourceTrigger=PropertyChanged}"
                         VerticalAlignment="Center"
                         HorizontalAlignment="Left"
                         Margin="10,1,10,-6"
                         Grid.Column="1"
                         Style="{StaticResource ModernInputbox}" 
                         />
            </Border>

        </StackPanel>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal" RenderTransformOrigin="0.691,0.515" Margin="0,10,0,0" Height="30">
            <Border Width="100">
